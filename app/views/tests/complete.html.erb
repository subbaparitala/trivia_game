<div class="container">
  <br>
  <br>
  <br>
  <br>
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="text-center">
          <small>Completed <b></b></small>
          </h3>
        </div>
        <div class="panel-body">
        	<ul class="list-group">
        	<% @test_datums.each do |test_data| %>
        	<li class="list-group-item"><%= "Question "+test_data.question_id.to_s%>: <%= test_answer test_data.answer_id %> <%= check_answer test_data.answer_id,test_data.question_id %></li>
        	<%end%>
        	<li class="list-group-item">Total Marks : <%= @test.total_marks.to_i %> / <%= @test_datums.count*4 %></li>
        </ul>
        </div>
        </div>
      </div>
    </div>
</div>